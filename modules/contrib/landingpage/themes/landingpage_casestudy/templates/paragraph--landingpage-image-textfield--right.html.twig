{%
set classes = [
'paragraph',
'paragraph--type--' ~ paragraph.bundle|clean_class,
view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}
{{ content.field_paragraph_anchor[0] }}
<div class="project_logo">{{ content.field_landingpage_image[0] }}</div>
{% if inline_styles %}
<div{{ attributes.addClass(classes) }} style="{{ inline_styles }}">
{% else %}
<div{{ attributes.addClass(classes) }}>
{% endif %}
  <div class="container">
	<div class="row">   
	  <div class="col-sm-12">
	    <h1>{{ content.field_landingpage_textfield[0] }}</h1>
	  </div>
	</div>
  </div>	  
</div>